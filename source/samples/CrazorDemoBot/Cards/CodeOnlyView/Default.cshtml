@using CrazorDemoBot.Cards.CodeOnlyView
@inherits CardView
<Card Version="1.5">

    <TextBlock>This card demonstrates defining a view which is only code (no razor)</TextBlock>
    <Action.Execute Verb="@nameof(OnShowMyCard)" Title="Show Code Only View"/>
</Card>
@functions {
    public void OnShowMyCard()
    {
        ShowView<MyCodeView>();
    }
}