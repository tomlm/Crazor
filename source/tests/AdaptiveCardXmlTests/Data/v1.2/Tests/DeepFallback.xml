<?xml version="1.0" encoding="utf-16"?>
<Card xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Version="1.2">
  <Container>
    <Fallback>
      <Type>drop</Type>
    </Fallback>
    <Container>
      <ColumnSet>
        <Column Width="auto">
          <TextBlock Weight="bolder" Color="attention" Text="You should never see this (fallback: drop)!" />
          <AdaptiveUnknownElement Id="firstGraph">
            <AdditionalProperties>
              <item>
                <key>
                  <string>someProperty</string>
                </key>
                <value>
                  <anyType xsi:type="xsd:string">blah</anyType>
                </value>
              </item>
            </AdditionalProperties>
            <Type>Graph</Type>
          </AdaptiveUnknownElement>
        </Column>
      </ColumnSet>
    </Container>
  </Container>
  <TextBlock Weight="lighter" Text="Nothing above this! ↑" />
  <TextBlock Weight="lighter" Text="Message just below ↓" />
  <Container>
    <Fallback>
      <Type>content</Type>
      <Content xsi:type="TextBlock" Weight="bolder" Color="good" Text="If you see this, things are working great!" />
    </Fallback>
    <Container>
      <ColumnSet>
        <Column Width="auto">
          <TextBlock Weight="bolder" Color="attention" Text="You should never see this (fallback: textblock)!" />
          <AdaptiveUnknownElement Id="secondGraph">
            <AdditionalProperties>
              <item>
                <key>
                  <string>someOtherProperty</string>
                </key>
                <value>
                  <anyType xsi:type="xsd:string">blah</anyType>
                </value>
              </item>
            </AdditionalProperties>
            <Type>Graph</Type>
          </AdaptiveUnknownElement>
        </Column>
      </ColumnSet>
    </Container>
  </Container>
  <TextBlock Weight="lighter" Text="There should be another message below ↓" />
  <AdaptiveUnknownElement Id="firstGraphVTwo">
    <Fallback>
      <Type>content</Type>
      <Content xsi:type="AdaptiveUnknownElement" Id="thirdGraph">
        <Fallback>
          <Type>content</Type>
          <Content xsi:type="TextBlock" Weight="bolder" Color="good" Text="Multiply-nested fallback works!" />
        </Fallback>
        <Type>Graph</Type>
      </Content>
    </Fallback>
    <Type>GraphV2</Type>
  </AdaptiveUnknownElement>
  <Container Style="emphasis">
    <TextBlock Weight="lighter" Text="There should be nothing between here..." Wrap="true" />
    <AdaptiveUnknownElement Id="secondGraphVTwo">
      <Fallback>
        <Type>content</Type>
        <Content xsi:type="AdaptiveUnknownElement" Id="fourthGraph">
          <Fallback>
            <Type>content</Type>
            <Content xsi:type="AdaptiveUnknownElement">
              <Type>MakeSomethingUp</Type>
            </Content>
          </Fallback>
          <Type>Graph</Type>
        </Content>
      </Fallback>
      <Type>GraphV2</Type>
    </AdaptiveUnknownElement>
    <AdaptiveUnknownElement Id="thirdGraphVTwo">
      <Fallback>
        <Type>content</Type>
        <Content xsi:type="AdaptiveUnknownElement" Id="fifthGraph">
          <Fallback>
            <Type>content</Type>
            <Content xsi:type="AdaptiveUnknownElement">
              <Fallback>
                <Type>drop</Type>
              </Fallback>
              <Type>MakeSomethingElseUp</Type>
            </Content>
          </Fallback>
          <Type>Graph</Type>
        </Content>
      </Fallback>
      <Type>GraphV2</Type>
    </AdaptiveUnknownElement>
    <TextBlock Weight="lighter" Text="...and here" Wrap="true" />
  </Container>
  <AdaptiveUnknownAction Title="Be Cool">
    <AdditionalProperties>
      <item>
        <key>
          <string>coolness</string>
        </key>
        <value>
          <anyType xsi:type="xsd:string">to the max</anyType>
        </value>
      </item>
    </AdditionalProperties>
    <Fallback>
      <Type>content</Type>
      <Content xsi:type="AdaptiveUnknownElement">
        <AdditionalProperties>
          <item>
            <key>
              <string>coolness</string>
            </key>
            <value>
              <anyType xsi:type="xsd:string">pretty cool</anyType>
            </value>
          </item>
          <item>
            <key>
              <string>title</string>
            </key>
            <value>
              <anyType xsi:type="xsd:string">Pretty cool</anyType>
            </value>
          </item>
        </AdditionalProperties>
        <Fallback>
          <Type>content</Type>
          <Content xsi:type="Action.Submit" Title="Correct" />
        </Fallback>
        <Type>MyCoolAction</Type>
      </Content>
    </Fallback>
    <Type>MyCoolAction2</Type>
  </AdaptiveUnknownAction>
  <AdaptiveUnknownAction Title="This is a bug!">
    <Fallback>
      <Type>drop</Type>
    </Fallback>
    <Type>Unnecessary</Type>
  </AdaptiveUnknownAction>
</Card>