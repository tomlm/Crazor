<?xml version="1.0" encoding="utf-16"?>
<Card xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Version="1.2">
  <Container>
    <Fallback Type="drop" />
    <Container>
      <ColumnSet>
        <Column Width="auto">
          <TextBlock Weight="bolder" Color="attention" Text="You should never see this (fallback: drop)!" />
          <UnknownElement Id="firstGraph" Type="Graph">
            <AdditionalProperties>
              <Property Name="someProperty">blah</Property>
            </AdditionalProperties>
          </UnknownElement>
        </Column>
      </ColumnSet>
    </Container>
  </Container>
  <TextBlock Weight="lighter" Text="Nothing above this! ↑" />
  <TextBlock Weight="lighter" Text="Message just below ↓" />
  <Container>
    <Fallback>
      <TextBlock Weight="bolder" Color="good" Text="If you see this, things are working great!" />
    </Fallback>
    <Container>
      <ColumnSet>
        <Column Width="auto">
          <TextBlock Weight="bolder" Color="attention" Text="You should never see this (fallback: textblock)!" />
          <UnknownElement Id="secondGraph" Type="Graph">
            <AdditionalProperties>
              <Property Name="someOtherProperty">blah</Property>
            </AdditionalProperties>
          </UnknownElement>
        </Column>
      </ColumnSet>
    </Container>
  </Container>
  <TextBlock Weight="lighter" Text="There should be another message below ↓" />
  <UnknownElement Id="firstGraphVTwo" Type="GraphV2">
    <Fallback>
      <UnknownElement Id="thirdGraph" Type="Graph">
        <Fallback>
          <TextBlock Weight="bolder" Color="good" Text="Multiply-nested fallback works!" />
        </Fallback>
      </UnknownElement>
    </Fallback>
  </UnknownElement>
  <Container Style="emphasis">
    <TextBlock Weight="lighter" Text="There should be nothing between here..." Wrap="true" />
    <UnknownElement Id="secondGraphVTwo" Type="GraphV2">
      <Fallback>
        <UnknownElement Id="fourthGraph" Type="Graph">
          <Fallback>
            <UnknownElement Type="MakeSomethingUp" />
          </Fallback>
        </UnknownElement>
      </Fallback>
    </UnknownElement>
    <UnknownElement Id="thirdGraphVTwo" Type="GraphV2">
      <Fallback>
        <UnknownElement Id="fifthGraph" Type="Graph">
          <Fallback>
            <UnknownElement Type="MakeSomethingElseUp">
              <Fallback Type="drop" />
            </UnknownElement>
          </Fallback>
        </UnknownElement>
      </Fallback>
    </UnknownElement>
    <TextBlock Weight="lighter" Text="...and here" Wrap="true" />
  </Container>
  <AdaptiveUnknownAction Title="Be Cool" Type="MyCoolAction2">
    <AdditionalProperties>
      <Property Name="coolness">to the max</Property>
    </AdditionalProperties>
    <Fallback>
      <UnknownElement Type="MyCoolAction">
        <AdditionalProperties>
          <Property Name="coolness">pretty cool</Property>
          <Property Name="title">Pretty cool</Property>
        </AdditionalProperties>
        <Fallback>
          <Action.Submit Title="Correct" />
        </Fallback>
      </UnknownElement>
    </Fallback>
  </AdaptiveUnknownAction>
  <AdaptiveUnknownAction Title="This is a bug!" Type="Unnecessary">
    <Fallback Type="drop" />
  </AdaptiveUnknownAction>
</Card>