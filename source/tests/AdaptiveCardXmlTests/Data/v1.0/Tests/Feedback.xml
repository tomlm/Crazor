<?xml version="1.0" encoding="utf-16"?>
<Card xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Version="1.0">
  <TextBlock Size="large" Weight="bolder" Text="Please rate your experience" />
  <Input.ChoiceSet Id="Rating" Spacing="medium" Value="5">
    <AdditionalProperties>
      <item>
        <key>
          <string>style</string>
        </key>
        <value>
          <anyType xsi:type="xsd:string">compact</anyType>
        </value>
      </item>
    </AdditionalProperties>
    <Choices>
      <Choice Title="Excellent" Value="5" />
      <Choice Title="Good" Value="4" />
      <Choice Title="Average" Value="3" />
      <Choice Title="Poor" Value="2" />
      <Choice Title="Very poor" Value="1" />
    </Choices>
  </Input.ChoiceSet>
  <TextBlock Spacing="large" Text="Leave a comment" />
  <Input.Text Id="FeedbackText" Spacing="medium" IsMultiline="true" MaxLength="500" />
  <TextBlock Spacing="small" Text="%20">
    <AdditionalProperties>
      <item>
        <key>
          <string>space</string>
        </key>
        <value>
          <anyType xsi:type="xsd:string">preserve</anyType>
        </value>
      </item>
    </AdditionalProperties>
  </TextBlock>
  <Action.Submit Title="Send">{
  "CardType": "FeedbackCard",
  "DialogId": "Feedback",
  "Command": "SendFeedback",
  "ActionAvailability": "GlobalInterrupt"
}</Action.Submit>
</Card>