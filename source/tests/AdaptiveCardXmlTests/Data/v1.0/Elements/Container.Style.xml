<?xml version="1.0" encoding="utf-16"?>
<Card xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Version="1.0">
  <AdditionalProperties>
    <item>
      <key>
        <string>style</string>
      </key>
      <value>
        <anyType xsi:type="xsd:string">emphasis</anyType>
      </value>
    </item>
  </AdditionalProperties>
  <TextBlock Text="The card has the emphasis style" />
  <Container Style="Default">
    <TextBlock Text="This container has the default style" />
    <TextBlock Text="color: default" />
    <TextBlock Color="Accent" Text="color: accent" />
    <TextBlock Color="Good" Text="color: good" />
    <TextBlock Color="Warning" Text="color: warning" />
    <TextBlock Color="Attention" Text="color: attention" />
    <TextBlock Color="Light" Text="color: light" />
    <TextBlock Color="Dark" Text="color: dark" />
  </Container>
  <Container Style="Emphasis">
    <TextBlock Text="This container has the emphasis style" />
    <TextBlock Text="color: default" />
    <TextBlock Color="Accent" Text="color: accent" />
    <TextBlock Color="Good" Text="color: good" />
    <TextBlock Color="Warning" Text="color: warning" />
    <TextBlock Color="Attention" Text="color: attention" />
    <TextBlock Color="Light" Text="color: light" />
    <TextBlock Color="Dark" Text="color: dark" />
    <Container>
      <TextBlock Color="Dark" Text="Contained container - no style" />
    </Container>
    <Container Style="Emphasis">
      <TextBlock Color="Dark" Text="Contained container - emphasis" />
    </Container>
    <Container Style="Default">
      <TextBlock Color="Dark" Text="Contained container - default" />
    </Container>
  </Container>
</Card>