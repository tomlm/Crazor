<Card xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Version="1.0">
  <TextBlock Size="Medium" Text="Spacing controls the amount of space between this element and the one before it" Wrap="true" />
  <TextBlock Spacing="None" Text="spacing: none" />
  <TextBlock Spacing="Small" Text="spacing: small" />
  <TextBlock Text="spacing: default" />
  <TextBlock Spacing="Medium" Text="spacing: medium" />
  <TextBlock Spacing="Large" Text="spacing: large" />
  <TextBlock Spacing="ExtraLarge" Text="spacing: extraLarge" />
